  <script>
    $('#dataTable').on('click', '.button-update', function() {
      let id = $(this).data('id');
      let nama = $(this).data('nama');
      let unker = $(this).data('unker');
      let nik = $(this).data('nik');
      let npwp = $(this).data('npwp');
      let no_rek_jateng = $(this).data('no_rek_jateng');
      let no_rek_bni = $(this).data('no_rek_bni');
      let no_bpjs_kes = $(this).data('no_bpjs_kes');
      let no_bpjs_ket = $(this).data('no_bpjs_ket');

      $('#edit-modal').modal('show');
      $('.id').val(id);
      $('.nama').val(nama);
      $('.unker').val(unker);
      $('.nik').val(nik);
      $('.npwp').val(npwp);
      $('.no_rek_jateng').val(no_rek_jateng);
      $('.no_rek_bni').val(no_rek_bni);
      $('.no_bpjs_kes').val(no_bpjs_kes);
      $('.no_bpjs_ket').val(no_bpjs_ket);
    })
  </script>